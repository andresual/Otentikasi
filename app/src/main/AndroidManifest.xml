<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andresual.dev.otentikasi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="Register SiBling" />
        <activity
            android:name=".LoginActivity"
            android:label="Login SiBling" />
        <activity
            android:name=".ProfileActivity"
            android:label="SiBling" />
        <activity android:name=".AirActivity" />
        <activity
            android:name=".BookingActivity"
            android:label="Booking Sampling" />
        <activity android:name=".SaranActivity" />
        <activity
            android:name=".KonfirmBookingActivity"
            android:label="Konfirmasi Pembayaran" />
        <activity android:name=".coba" />
        <activity android:name=".coba2"></activity>
    </application>

</manifest>